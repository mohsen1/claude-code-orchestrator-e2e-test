{
  "name": "calculator-project",
  "version": "1.0.0",
  "description": "A TypeScript calculator application for testing tmux-based Claude runner",
  "main": "dist/calculator.js",
  "types": "dist/calculator.d.ts",
  "scripts": {
    "build": "tsc",
    "build:prod": "tsc --project tsconfig.build.json",
    "clean": "rm -rf dist",
