    "validate": "bash validate.sh",
    "build-and-test": "bash build.sh && bash validate.sh"
  },
  "keywords": [
    "calculator",
    "typescript",
    "math"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
